{
  "name": "beach-volley-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@apollo/client": "^3.3.9",
    "@graphile-contrib/pg-simplify-inflector": "^6.1.0",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.3",
    "@testing-library/user-event": "^12.6.2",
    "cors": "^2.8.5",
    "date-fns": "^2.17.0",
    "env-cmd": "^10.1.0",
    "express": "^4.17.1",
    "formik": "^2.2.6",
    "graphile-migrate": "^1.0.2",
    "graphql": "^15.5.0",
    "husky": "^5.0.9",
    "lint-staged": "^10.5.4",
    "postgraphile": "^4.10.0",
    "prettier": "^2.2.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.1",
    "styled-components": "^5.2.1",
    "uuid4": "^2.0.2",
    "web-vitals": "^0.2.4",
    "yup": "^0.32.8"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "graphile": "postgraphile --cors --subscriptions --watch --dynamic-json --no-setof-functions-contain-nulls --no-ignore-rbac --no-ignore-indexes --show-error-stack=json --extended-errors hint,detail,errcode --append-plugins @graphile-contrib/pg-simplify-inflector --export-schema-graphql data/schema.graphql --graphiql \"/\" --enhance-graphiql --allow-explain --enable-query-batching --legacy-relations omit --connection \"postgres://beachvolley_graphile:dev_password@localhost:5432/beachvolley\" --owner-connection \"postgres://beachvolley_graphile_superuser:dev_password@localhost:5432/beachvolley\" --default-role beachvolley_graphile_anonymous --schema beachvolley_public",
    "server": "node server.js",
    "migrations": "env-cmd graphile-migrate"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ]
  }
}
